# Clé API 0x - Obtenez la vôtre sur 0x.org
ZEROX_API_KEY=votre_cle_api_0x_ici

# Clé privée de votre wallet (GARDEZ-LA SECRÈTE!)
PRIVATE_KEY=0xvotre_cle_privee_ici

# Adresse de votre wallet
TAKER_ADDRESS=0xvotre_adresse_wallet_ici

# URL RPC pour Base (optionnel)
RPC_URL=https://mainnet.base.org

# Token du bot Telegram
TELEGRAM_BOT_TOKEN=votre_token_telegram_ici

